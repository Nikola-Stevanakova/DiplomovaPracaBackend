<?xml version="1.0" encoding="UTF-8"?><databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <changeSet author="admin" id="8e216e86-278d-4ffa-8a26-ec243ccacf51">
        <createTable tableName="book_person">
            <column name="book_id" type="char(24)"/>
            <column name="person_id" type="char(24)"/>
        </createTable>
        <createTable tableName="book_classroom">
            <column name="book_id" type="char(24)"/>
            <column name="classroom_id" type="char(24)"/>
        </createTable>
        <createTable tableName="book">
            <column name="id" type="char(24)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="title" type="varchar(50)"/>
        </createTable>
        <createTable tableName="classroom">
            <column name="id" type="char(24)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="number" type="varchar(50)"/>
        </createTable>
        <createTable tableName="main_form">
            <column name="id" type="char(24)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="title" type="varchar(50)"/>
            <column name="enumeration_select" type="varchar(50)"/>
            <column name="enumeration_map_select" type="varchar(50)"/>
            <column name="multichoice_select" type="varchar(50) ARRAY"/>
            <column name="multichoice_map_select" type="varchar(50) ARRAY"/>
            <column name="count_select" type="numeric"/>
            <column name="boolean_field" type="boolean"/>
            <column name="date_select" type="date"/>
            <column name="date_time_select" type="timestamp"/>
            <column name="file_select" type="varchar(50)"/>
            <column name="file_list_select" type="varchar(50) ARRAY"/>
            <column name="user_select" type="varchar(50)"/>
            <column name="user_list_select" type="varchar(50) ARRAY"/>
            <column name="vehicle_header" type="varchar(255)"/>
            <column name="vehicle_id" type="char(24)"/>
            <column name="vehicle_form" type="varchar(50) ARRAY"/>
        </createTable>
        <createTable tableName="password">
            <column name="id" type="char(24)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="person_id" type="char(24)"/>
        </createTable>
        <createTable tableName="person">
            <column name="id" type="char(24)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="name" type="varchar(50)"/>
            <column name="surname" type="varchar(50)"/>
            <column name="tel_num" type="varchar(50)"/>
            <column name="vehicle_id" type="char(24)"/>
            <column name="password_id" type="char(24)"/>
            <column name="classroom_id" type="char(24)"/>
            <column name="new_process_version" type="varchar(50)"/>
            <column name="book_list_header" type="varchar(255)"/>
            <column name="vehicle_header" type="varchar(255)"/>
            <column name="vehicle_form" type="varchar(50) ARRAY"/>
        </createTable>
        <createTable tableName="vehicle">
            <column name="id" type="char(24)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="type" type="varchar(50)"/>
            <column name="brand" type="varchar(50)"/>
            <column name="color" type="varchar(50)"/>
            <column name="seat_count" type="numeric"/>
            <column name="person_id" type="char(24)"/>
            <column name="person_form" type="varchar(50) ARRAY"/>
            <column name="main_form_id" type="char(24)"/>
        </createTable>
    </changeSet>
    <changeSet author="admin" id="3dbe34a2-ca95-40f3-a65a-4f56de7d796d"/>
    <changeSet author="admin" id="6e427571-8c37-4e27-a90b-c91eeced3ddd"/>
    <changeSet author="admin" id="b25d837d-4a25-4e2e-946d-e3407f79c27c">
        <addForeignKeyConstraint baseColumnNames="book_id" baseTableName="book_person" constraintName="book_id" referencedColumnNames="id" referencedTableName="book"/>
        <addForeignKeyConstraint baseColumnNames="person_id" baseTableName="book_person" constraintName="person_id" referencedColumnNames="id" referencedTableName="person"/>
        <addForeignKeyConstraint baseColumnNames="book_id" baseTableName="book_classroom" constraintName="book_id" referencedColumnNames="id" referencedTableName="book"/>
        <addForeignKeyConstraint baseColumnNames="classroom_id" baseTableName="book_classroom" constraintName="classroom_id" referencedColumnNames="id" referencedTableName="classroom"/>
        <addForeignKeyConstraint baseColumnNames="vehicle_id" baseTableName="main_form" constraintName="vehicle_id" referencedColumnNames="id" referencedTableName="vehicle"/>
        <addForeignKeyConstraint baseColumnNames="person_id" baseTableName="password" constraintName="person_id" referencedColumnNames="id" referencedTableName="person"/>
        <addForeignKeyConstraint baseColumnNames="vehicle_id" baseTableName="person" constraintName="vehicle_id" referencedColumnNames="id" referencedTableName="vehicle"/>
        <addForeignKeyConstraint baseColumnNames="password_id" baseTableName="person" constraintName="password_id" referencedColumnNames="id" referencedTableName="password"/>
        <addForeignKeyConstraint baseColumnNames="classroom_id" baseTableName="person" constraintName="classroom_id" referencedColumnNames="id" referencedTableName="classroom"/>
        <addForeignKeyConstraint baseColumnNames="person_id" baseTableName="vehicle" constraintName="person_id" referencedColumnNames="id" referencedTableName="person"/>
        <addForeignKeyConstraint baseColumnNames="main_form_id" baseTableName="vehicle" constraintName="main_form_id" referencedColumnNames="id" referencedTableName="main_form"/>
    </changeSet>
</databaseChangeLog>
