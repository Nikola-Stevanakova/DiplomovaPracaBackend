<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://petriflow.com/petriflow.schema.xsd">
	<id>main_form</id>
	<initials>PMF</initials>
	<title>Hlavný formulár</title>
	<icon>device_hub</icon>
	<defaultRole>true</defaultRole>
	<anonymousRole>true</anonymousRole>
	<transitionRole>false</transitionRole>

	<caseEvents>
		<event type="create">
			<id>main_form_create_vehicle_case</id>
			<actions phase="post">
				<action>
					vehicle_id: f.vehicle_id,
					vehicle_form: f.vehicle_form;

					def vehicleCase = createCase("vehicle")
					String taskId = vehicleCase.tasks.find { it.transition == "t1" }.task

					change vehicle_id value { [vehicleCase.stringId] }
					change vehicle_form value { [taskId] }
				</action>
			</actions>
		</event>
	</caseEvents>

	<role>
		<id>system</id>
		<title>System</title>
	</role>

	<data type="button">
		<id>create_update_process_instance</id>
		<title/>
		<placeholder>Nahrajte údaje</placeholder>
		<component>
			<name>stroked</name>
		</component>
		<event type="set">
			<id>add_new_process_set</id>
			<actions phase="post">
				<action>
					updateOrInsert()
				</action>
			</actions>
		</event>
	</data>
	<data type="text">
		<id>title</id>
		<title>Názov</title>
		<init>Názov hlavného formulára</init>
	</data>
	<data type="enumeration">
		<id>enumeration_select</id>
		<title>Výber zo zoznamu enumerácie</title>
		<options>
			<option key="hodnota 1">hodnota 1</option>
			<option key="hodnota 2">hodnota 2</option>
		</options>
		<init>hodnota 1</init>
	</data>
	<data type="enumeration_map">
		<id>enumeration_map_select</id>
		<title>Výber zo zoznamu enumeračnej mapy</title>
		<options>
