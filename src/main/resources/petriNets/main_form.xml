			<option key="key2">hodnota 2</option>
		</options>
		<init>key2</init>
	</data>
	<data type="multichoice">
		<id>multichoice_select</id>
		<title>Výber zo zoznamu multichoice</title>
		<options>
			<option key="hodnota1">hodnota1</option>
			<option key="hodnota2">hodnota2</option>
			<option key="hodnota3">hodnota3</option>
			<option key="hodnota4">hodnota4</option>
		</options>
		<inits>
			<init>hodnota1</init>
			<init>hodnota2</init>
		</inits>
	</data>
	<data type="multichoice_map">
		<id>multichoice_map_select</id>
