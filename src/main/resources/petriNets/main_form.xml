		<options>
			<option key="key1">hodnota1</option>
			<option key="key2">hodnota2</option>
			<option key="key3">hodnota3</option>
			<option key="key4">hodnota4</option>
		</options>
		<inits>
			<init>key2</init>
			<init>key3</init>
			<init>key4</init>
		</inits>
	</data>
	<data type="number">
		<id>count_select</id>
